<Window x:Class="BankApp.GraphicalUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BankApp.GraphicalUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid>
            <StackPanel>
                <Button Content="Login" Click="LoginButton_Click"/>
                <Button Content="Create Account" Click="CreateAccountButton_Click"/>
                <Button Content="Exit" Click="ExitButton_Click"/>
            </StackPanel>
        </Grid>

    </Grid>
</Window>
    
